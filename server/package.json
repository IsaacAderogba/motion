{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node -P ./tsconfig.json --transpile-only src/index.ts",
    "dev": "ts-node-dev -P ./tsconfig.json --no-notify --transpileOnly --respawn ./src",
    "build": "ts-node -P ./tsconfig.json --log-error src/index.ts",
    "debug": "ts-node-dev -P ./tsconfig.json --inspect-brk --no-notify --transpileOnly --respawn ./src",
    "knex:migrate:make": "knex --knexfile src/db/knexfile.js migrate:make",
    "knex:migrate:latest": "knex --knexfile src/db/knexfile.js migrate:latest",
    "knex:migrate:rollback": "knex --knexfile src/db/knexfile.js migrate:rollback"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nexus/schema": "^0.13.1",
    "apollo-datasource-rest": "^0.8.1",
    "apollo-server-express": "^2.12.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "datasource-sql": "^1.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.0.0",
    "graphql-middleware": "^4.0.2",
    "helmet": "^3.22.0",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.21.0",
    "nodemon": "^2.0.3",
    "objection": "^2.1.3",
    "pg": "^8.0.3",
    "ts-node": "^8.9.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
