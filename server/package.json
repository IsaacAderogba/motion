    "@types/helmet": "0.0.46",
    "@types/jsonwebtoken": "^8.3.9",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "eslint": "^6.8.0",
    "jest": "^25.4.0",
    "should": "^13.2.3",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
